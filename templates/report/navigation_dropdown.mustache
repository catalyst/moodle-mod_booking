{{!
    @template mod_booking/report/navigation_dropdown

    Classes required for JS:
    * none

    Data attributes required for JS:
    * data-region

    Example context (json):
    {
        "prettydate" : "Mo, 28.01.2025, 12:00-13:00",
		"dateurl" : "https://www.exampleurl.xyz"
    }
}}
{{#optiondatesexist}}
<span class="mt-1">
    <select id="mod-booking-report2-navigation-dropdown"
        onchange="M.mod_booking.bookingjslib.navigateToPage(this)" class="form-control">
        {{#optiondates}}
        <option value="{{dateurl}}">{{prettydate}}</option>
        {{/optiondates}}
    </select>
</span>
{{/optiondatesexist}}
